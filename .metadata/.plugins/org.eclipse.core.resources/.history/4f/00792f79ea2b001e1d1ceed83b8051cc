import java.util.Scanner;

public class Main {
	
	public static int Exercicio1(int num1, int num2) {
		int num1,num2,soma=0;
		Scanner sc = new Scanner(System.in);
		System.out.print("Digite o primeiro e o segundo número separados por espaços para fazer uma soma: ");
		num1 = sc.nextInt();
		num2= sc.nextInt();
		
		
		return soma;
	}


	public static void main(String[] args) {
		int soma,opcao;


		do {
			System.out.println("MENU DE EXERCÍCIOS: ");
			System.out.println("1 - Soma");
			System.out.println("2 - Raio");
			System.out.println("MENU DE EXERCÍCIOS: ");
			System.out.println("MENU DE EXERCÍCIOS: ");
			System.out.println("MENU DE EXERCÍCIOS: ");
			System.out.println("MENU DE EXERCÍCIOS: ");
			
			
		}while(opcao!=7);

		
		soma= Exercicio1(num1,num2);
		
		System.out.println("Soma: "+soma);
	}
	
}
